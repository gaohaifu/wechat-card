<view class="content"><user-info vue-id="2b5cccba-1" userData="{{userData}}" color="{{color}}" bind:__l="__l"></user-info><view class="newTitle flex_layout"><view class="newTitle_item newTitle_active" style="{{'color:'+(color)+';'+('border-color:'+(color)+';')}}">最新</view></view><view class="trends_content"><block wx:if="{{$root.g0>0}}"><block><block wx:for="{{trendsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['detail',['$0'],[[['trendsList','',index,'id']]]]]]]}}" class="trends_item flex_layout" bindtap="__e"><view class="img_content"><image src="{{cdnUrl+item.picimages[0]}}" mode="aspectFill"></image></view><view class="text_content"><view class="image_title">{{item.title}}</view><view class="maincontent">{{item.intro}}</view><view class="thumb_up flex_layout"><button class="right_thumb flex_layout" open-type="share" id="{{item.id}}" data-title="{{item.title}}" data-imgUrl="{{item.picimages[0]}}"><view class="iconView iconfont icon-fenxiang"></view><text>分享</text></button><view class="right_thumb flex_layout"><block wx:if="{{item.isfavor}}"><block><view data-event-opts="{{[['tap',[['helpBtn',[index,'$0','$1'],[[['trendsList','',index,'id']],[['trendsList','',index,'isfavor']]]]]]]}}" class="iconView iconfont icon-thumbup-fill" style="{{'color:'+(color)+';'}}" catchtap="__e"></view></block></block><block wx:else><block><view data-event-opts="{{[['tap',[['helpBtn',[index,'$0','$1'],[[['trendsList','',index,'id']],[['trendsList','',index,'isfavor']]]]]]]}}" class="iconView iconfont icon-dianzan1" catchtap="__e"></view></block></block><text style="{{'color:'+(item.isfavor?color:'')+';'}}">{{item.favorNum}}</text></view><block wx:if="{{usertype==1}}"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['trendsList','',index,'id']]]]]]]}}" class="edit_button" catchtap="__e"><button style="{{'border-color:'+(color)+';'+('color:'+(color)+';')}}">编辑信息</button></view></block></view></view></view></block></block></block><block wx:else><block><block wx:if="{{reveal}}"><no-data vue-id="2b5cccba-2" bind:__l="__l"></no-data></block></block></block></view><block wx:if="{{usertype==1}}"><block><view style="height:100rpx;"></view><view data-event-opts="{{[['tap',[['addBtn',['$event']]]]]}}" class="add_btn" bindtap="__e"><button style="{{'background:'+(color)+';'}}">+ 新增企业动态</button></view></block></block></view>