.imageContainer {
  width: 100%;
  /* height: 500rpx; */
  /* background: #000; */
  height: 100%;
}
.swiperitem {
  padding: 0 15rpx;
  box-sizing: border-box;
}
.swiperitem view.swiperitem_view {
  background: #fff;
  padding: 80rpx 100rpx 30rpx;
  border-radius: 10px;
}
.swiperitem view.swiperitem_view view {
  color: #333;
  font-size: 32rpx;
  text-align: center;
  margin-top: 50rpx;
}
.swiperitem view.swiperitem_view text {
  display: block;
  color: #666;
  font-size: 24rpx;
  text-align: center;
  margin-top: 20rpx;
}
.swiperitem view.swiperitem_view button {
  width: 225rpx;
  height: 75rpx;
  line-height: 75rpx;
  margin: 20rpx auto 0;
  display: block;
  background: #0084bf;
  color: #fff;
  font-size: 24rpx;
  border-radius: 5px;
}
.itemImg {
  width: 100%;
  display: block;
  /* height: 380rpx; */
  border-radius: 15rpx;
  box-shadow: 0px 4rpx 15rpx 0px rgba(153, 153, 153, 0.24);
}
.swiperactive {
  width: 95%;
  opacity: 1;
  z-index: 10;
  /* height: 430rpx; */
  height: 287rpx;
  top: 0%;
  transition: all .2s ease-in 0s;
}
.zhankai {
  text-align: center;
}
.zhankai .iconfont {
  margin-left: 10rpx;
}

